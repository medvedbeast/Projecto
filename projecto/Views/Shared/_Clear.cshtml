<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8" lang="en-us" />
    <title>@ViewBag.Title</title>
    <link href="~/css/common.css" rel="stylesheet" type="text/css" />
    @if (ViewBag.Stylesheet != null)
    {
        <link href="~/css/@ViewBag.Stylesheet" rel="stylesheet" type="text/css" />
    }
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed" rel="stylesheet">
    <script src="~/lib/jquery/dist/jquery.slim.js" type="text/javascript"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script src="~/js/common.js" type="text/javascript"></script>
    @if (ViewBag.Script != null)
    {
        <script src="~/js/@ViewBag.Script" type="text/javascript"></script>
    }
</head>
<body>
    @RenderBody()
</body>
</html>
